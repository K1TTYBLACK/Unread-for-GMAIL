{
    "name": "Unread Tab for Gmail",
    "version": "1.0.2",
    "description": "Adds UNREAD tab to Gmail where you can only see unread mails.",
    "manifest_version": 3,
    "author": "BobVolskiy",
    "icons":{
        "16":"icon.png",
        "48":"icon.png",
        "128":"icon.png"
    },
    "content_scripts":[
        {
            "matches":["https://mail.google.com/*"],
            "js": ["script.js"]
        }
    ]
}